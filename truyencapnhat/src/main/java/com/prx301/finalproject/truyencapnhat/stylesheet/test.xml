<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="xml_config.xsl"?>
<p:crawler-config xmlns:p="http://t3.com/2018/xml/config">
    <p:components>
        <p:component>
            <p:name>Hako.re</p:name>
            <p:stylesheet>src/main/java/com/prx301/finalproject/truyencapnhat/stylesheet/hako_style.xsl</p:stylesheet>
            <p:config>src/main/java/com/prx301/finalproject/truyencapnhat/xmlConfigs/hako_project.xml</p:config>
            <p:clean-filters>
                <p:filter>
                    <p:page-uri>/danh-sach</p:page-uri>
                    <p:filter-patterns>
                        <p:pattern>&lt;section class="browse-section noheader-section col-12"&gt;[\s\S]+&lt;/footer&gt;[\s]+&lt;/section&gt;</p:pattern>
                    </p:filter-patterns>
                </p:filter>
                <p:filter>
                    <p:page-uri>/truyen/</p:page-uri>
                    <p:filter-patterns>
                        <p:pattern>&lt;article class="listall-item clear"&gt;[\S\s]+&lt;/div&gt;[\s]+&lt;/div&gt;[\s]+&lt;/article&gt;</p:pattern>
                        <p:pattern>&lt;div class="reg-part clear" style="padding-bottom: 20px"&gt;[\S\s]+&lt;/section&gt;[\s]{2,}&lt;/div&gt;[\s]+&lt;/div&gt;[\s]+&lt;/div&gt;</p:pattern>
                    </p:filter-patterns>
                </p:filter>
            </p:clean-filters>
            <p:status>inactive</p:status>
        </p:component>
        <p:component>
            <p:name>Novelupdate</p:name>
            <p:stylesheet>src/main/java/com/prx301/finalproject/truyencapnhat/stylesheet/novelupdate_style.xsl
            </p:stylesheet>
            <p:config>src/main/java/com/prx301/finalproject/truyencapnhat/xmlConfigs/novelupdate_project.xml</p:config>
            <p:uri-resolver>com.prx301.finalproject.truyencapnhat.utils.MALURIResolver</p:uri-resolver>
            <p:clean-filters>
                <p:filter>
                    <p:page-uri>/manga.php?q=&amp;type=2</p:page-uri>
                    <p:filter-patterns>
                        <p:pattern>&lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;[\s\S]+&lt;/table&gt;</p:pattern>
                        <p:pattern>&lt;div class="normal_header clearfix pt16"&gt;[\S\s]+Search Results&lt;/div&gt;
                        </p:pattern>
                    </p:filter-patterns>
                </p:filter>
                <p:filter>
                    <p:page-uri>/manga</p:page-uri>
                    <p:filter-patterns>
                        <p:pattern>&lt;h1 class="h1"&gt;&lt;span itemprop="name"&gt;[\S\s]+&lt;/h1&gt;</p:pattern>
                        <p:pattern>&lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;[\S\s]+&lt;/table&gt;</p:pattern>
                    </p:filter-patterns>
                </p:filter>
            </p:clean-filters>
            <p:status>active</p:status>
        </p:component>
    </p:components>
</p:crawler-config>